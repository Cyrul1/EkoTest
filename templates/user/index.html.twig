{% extends 'base.html.twig' %}

{% block body %}
    <h1>Twoje artykuły:</h1>

    <table>
        <thead>
            <tr>
                <th>Nazwa</th>
                <th>Jednostka miary</th>
                <th>Ilość Przyjęta</th>
                <th>Ilość wydana</th>
                <th>Cena</th>
                <th>VAT</th>
              
            </tr>
        </thead>
        <tbody>
            {% for artykul in artykuly %}
                <tr>
                    <td>{{ artykul.Nazwa }}</td>
                    <td>{{ artykul.JednostkaMiary }}</td>
                    <td>{{ artykul.IloscPrzyjeta }}</td>
                    <td>{{ artykul.IloscWydana }}</td>
                    <td>{{ artykul.Cena }}</td>
                    <td>{{ artykul.VAT }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
{% endblock %}